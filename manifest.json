{
  "name": "JoseB Chrome Extension",
  "version": "1",
  "manifest_version": 2,
  "icons": { "128": "assets/icon.png" },
  "background": {
    "persistent": false,
    "scripts": ["js/scripts.js"]
  },
  "permissions": ["http://mysafeinfo.com/*", "https://www.theguardian.com/*"],
  "web_accessible_resources": ["assets/ukeur.jpg", "*.ttf"],  
  "browser_action": {
    "default_title": "Jose Extension",
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
    "background": {
    "scripts": ["js/eventpage.js"],
    "persistent": false
  },
    "content_scripts": [
    {
      "matches": ["https://www.google.co.uk/?gfe_rd*"],
      "css": ["css/bootstrap.css","css/styles.css"],
      "js": ["js/lib/jquery.js", "js/eventpage.js"]
    }
  ]
}