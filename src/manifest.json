{
  "name": "JoseB Chrome Extension",
  "version": "1",
  "manifest_version": 2,
  "icons": { "128": "assets/icon.png" },
  "permissions": [
    "https://www.theguardian.com/*", 
    "https://rss.sciencedaily.com/top.xml", 
    "https://feeds.bbci.co.uk/news/rss.xml?edition=uk*", 
    "https://slashdot.org/slashdot.xml", 
    "https://www.reddit.com/r/PS4Deals/new/.xml*",
    "https://api.darksky.net/forecast/*"],
  "web_accessible_resources": [
    "assets/*",
    "js/*",
    "*.ttf"],  
  "browser_action": {
    "default_title": "Jose Extension",
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
    "background": {
    "scripts": ["js/third-party/axios.js", "js/third-party/jquery.js", "js/eventpage.js"],
    "persistent": false
  },
    "content_scripts": [
    {
      "matches": ["https://*/?gfe_rd*","https://www.google.com/"],
      "css": ["css/styles.css"],
      "js": [
        "js/third-party/axios.js",
        "js/third-party/jquery.js",
        "js/eventpage.js"
      ]
    }
  ]
}